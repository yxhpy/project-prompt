prefix=/usr/local/mxe/usr/x86_64-w64-mingw32.shared.all
includedir=${prefix}/include
libdir=${prefix}/lib

Name: Pango Win32
Description: Win32 GDI font support for Pango
Version: 1.56.4
Requires: pango
Requires.private: glib-2.0 >=  2.82, gobject-2.0 >=  2.82, gio-2.0 >=  2.82, fribidi >=  1.0.6, harfbuzz >=  8.4.0, fontconfig >=  2.15.0, freetype2, cairo >=  1.18.0
Libs: -L${libdir} -lpangowin32-1.0
Libs.private: -lm -lgdi32 -ldwrite
Cflags: -I${includedir}/pango-1.0
